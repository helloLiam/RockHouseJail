<view class="copy-container">
  <view class="topBar">
    <span class="copy-topBar-phonenticize">
      <image src="{{phoneticize}}" />
    </span>
    <span class="copy-topBar-shareButton"></span>
  </view>
  <view class="copy-mainFont"><image src="{{character}}" /></view>
  <view class="strokeSequence">
    <image src="{{strokeSequence}}" />
  </view>

  <view class="copy-canvas">
    
    <canvas id="myCanvas"
      type="2d"
      bindtouchstart="start"
      bindtouchmove="move"
      bindtouchend="end"/>

    <!-- <view hidden="{{hidden}}">
      Coordinates: ({{currentX}}, {{currentY}})
    </view> -->



  </view>
  






</view>